// Configure default options for autocomplete-rails gem
// Set Hebrew "no matches" message as default (from config/locales/he.yml :no_matches_found)
$(document).ready(function() {
  if (typeof $.railsAutocomplete !== 'undefined' && $.railsAutocomplete.options) {
    $.railsAutocomplete.options.noMatchesLabel = '<%= I18n.t(:no_matches_found) %>';
  }
});
